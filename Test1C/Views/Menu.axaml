<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Test1C.Menu"
			 xmlns:vm="using:Test1C.ViewModels"
			 x:DataType="vm:MenuViewModel">

	<Grid Margin="70 30">
		<Border BorderBrush="#30267D" BorderThickness="5" CornerRadius="20">
			<StackPanel>
				<Image Source="avares://Test1C/Assets/icon.png" Height="170"/>
				<Button Command="{Binding GoTems}" Content="Билеты" />
				<Button Command="{Binding GoErrors}" Content="Ошибки" />
				<Button Command="{Binding GoMarathon}" Content="Марафон" />
				<Button Command="{Binding GoExam}" Content="Экзамен" Background="#302679" Foreground="White"/>
			</StackPanel>
		</Border>
	</Grid>
	 
</UserControl>
